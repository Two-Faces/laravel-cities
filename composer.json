{
    "name": "two-faces/laravel-cities",
    "description": "Seed all countries/cities from geonames.org database. Searchable DB tree, ready to use API! Refactored for Laravel 10+ and PHP 8.2+",
    "keywords": ["laravel", "geonames.org", "geolocation", "countries", "cities", "nested-set", "geography"],
    "license": "MIT",
    "homepage": "https://github.com/Two-Faces/laravel_cities.git",
    "authors": [
        {
            "name": "Two Faces",
            "email": "anton98767@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-zip": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "TwoFaces\\LaravelCities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TwoFaces\\LaravelCities\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TwoFaces\\LaravelCities\\GeoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit",
        "pint": "pint",
        "pint:test": "pint --test",
        "phpstan": "phpstan analyse --memory-limit=2G",
        "analyse": "@phpstan"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

